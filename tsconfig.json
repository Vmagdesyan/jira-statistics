{
    "compilerOptions": {
        "outDir": "./dist/",
        "baseUrl": "./src",
        "noImplicitAny": false,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "paths": {
            "components/*": [
                "components/*"
            ],
        },
    },
    "include": [
        "src",
        "data/*",
        "./declaration.d.ts",
    ]
}